{
  "httpClient": {
    "type": "fabio",
    "retries": 3,
    "services": {
      "ads": "http://ads-service"
    }
  },
  "logger": {
    "console": {
      "enabled": true
    },
    "elk": {
      "enabled": false,
      "url": "http://elk:9200"
    },
    "file": {
      "enabled": false,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://seq:5341",
      "apiKey": "secret"
    }
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "ads",
    "udpHost": "jaeger",
    "udpPort": 6831,
    "maxPacketSize": 65000,
    "sampler": "const",
    "excludePaths": ["/", "/ping", "/metrics"]
  },
  "prometheus": {
    "enabled": true
  },
  "mongo": {
    "connectionString": "mongodb://mongo:27017",
    "seed": false
  },
  "rabbitMq": {
    "hostnames": [
      "rabbitmq"
    ]
  },
  "redis": {
    "connectionString": "redis",
    "instance": "ads:"
  },
  "vault": {
    "enabled": false,
    "url": "http://vault:8200",
    "kv": {
      "enabled": false
    },
    "pki": {
      "enabled": false
    },
    "lease": {
      "mongo": {
        "enabled": false
      }
    }
  }
}